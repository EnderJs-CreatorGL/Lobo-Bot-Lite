const handler = async (m, {conn, text, usedPrefix, command}) => {
  if (!text) throw `[â—] ğ—–ğ˜‚ğ—®ğ—¹ ğ—ºğ—²ğ—»ğ˜€ğ—®ğ—·ğ—² ğ—²ğ—»ğ˜ƒğ—¶ğ—¼ ğ—½ğ—®ğ—¿ğ—® ğ—°ğ—¼ğ—»ğ˜ğ—®ğ—°ğ˜ğ—®ğ—¿ ğ—®ğ—¹ ğ˜€ğ˜ğ—®ğ—³ğ—³\n\nğ—˜ğ—ğ—˜ğ— ğ—£ğ—Ÿğ—¢: ğš…ğ™´ğ™½ğ™¶ğ™°ğ™½ ğ™½ğ™´ğ™²ğ™´ğš‚ğ™¸ğšƒğ™¾ ğ™³ğ™´ ğš‚ğš„ ğ™°ğšˆğš„ğ™³ğ™° `;
  if (text.length < 10) throw `[â—] ğ—ºğ—²ğ—»ğ˜€ğ—®ğ—·ğ—² ğ—ºğ—¶ğ—»ğ—¶ğ—ºğ—¼ ğ—±ğ—² 10 ğ—°ğ—®ğ—¿ğ—®ğ—°ğ˜ğ—²ğ—¿`;
  if (text.length > 1000) throw `[â—] ğ—ºğ—²ğ—»ğ˜€ğ—®ğ—·ğ—² ğ—ºğ—®ğ˜…ğ—¶ğ—ºğ—¼ ğ—½ğ—®ğ—¿ğ—® ğ—°ğ—¼ğ—»ğ˜ğ—®ğ—°ğ˜ğ—®ğ—¿ ğ—±ğ—² 1000 ğœğšğ«ğšğœğ­ğğ«ğğ¬`;
  const teks = `*â’â•â•â•â•â•[â˜†ğ‘ğ„ğğğ‘ğ“ğ„â˜†]â•â•â•â•â•â’*\n*â”¬*\n*â”œâ§ ğ™½ğš„ğ™¼ğ™´ğšğ™¾:* wa.me/${m.sender.split`@`[0]}\n*â”´*\n*â”¬*\n*â”œâ§ ğ™¼ğ™´ğ™½ğš‚ğ™°ğ™¹ğ™´:* ${text}\n*â”´*`;
  conn.reply('51992004117@s.whatsapp.net', m.quoted ? teks + m.quoted.text : teks, null, {contextInfo: {mentionedJid: [m.sender]}});
  conn.reply('50576390682@s.whatsapp.net', m.quoted ? teks + m.quoted.text : teks, null, {contextInfo: {mentionedjid: [m.sender]}});
  conn.reply('527294888993@s.whatsapp.net',m.quoted ? teks + m.quoted.text : teks, null, {contextInfo: {mentionedjid: [m.sender]}});
  m.reply(`ğ—¦ğ—² ğ—°ğ—¼ğ—»ğ˜ğ—®ğ—°ğ˜ğ—¼ ğ—®ğ—¹ ğ˜€ğ˜ğ—®ğ—³ğ—³ ğ—±ğ—² ğ—Ÿğ—¢ğ—•ğ—¢-ğ—•ğ—¢ğ—§-ğ— ğ—— ğ—±ğ—² ğ—ºğ—®ğ—»ğ—²ğ—¿ğ—® ğ—²ğ˜…ğ—¶ğ˜ğ—¼ğ˜€ğ—®[ğŸº]`);
};
handler.help = ['reporte', 'request'].map((v) => v + ' <teks>');
handler.tags = ['info'];
handler.command = /^(reunion|emergensia|reunionstaff)$/i;
